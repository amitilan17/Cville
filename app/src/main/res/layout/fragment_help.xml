<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/last_backround"
    tools:context=".RegisterP3RefFragment">


    <ImageButton
        android:id="@+id/assaf"
        android:layout_width="299dp"
        android:layout_height="85dp"
        android:background="@drawable/g_20"
        android:text="@string/ukranian"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="200dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="186dp" />

    <ImageButton
        android:id="@+id/hotline"
        android:layout_width="304dp"
        android:layout_height="95dp"
        android:background="@drawable/g_1"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintTop_toBottomOf="@+id/assaf"
        android:layout_marginTop="30dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="306dp" />

    <ImageButton
        android:id="@+id/cimi"
        android:layout_width="304dp"
        android:layout_height="95dp"
        android:background="@drawable/g_21"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hotline"
        android:layout_marginTop="30dp"
        tools:layout_editor_absoluteY="442dp" />


    <TextView
        android:id="@+id/titleForm_p1_ref"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="Refugee Rights"
        android:textColor="@color/black"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>

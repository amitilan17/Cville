<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/last_backround"
    tools:context=".RegisterP3RefFragment">


    <ImageButton
        android:id="@+id/olim_habayta"
        android:layout_width="299dp"
        android:layout_height="85dp"
        android:background="@drawable/info_button"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="200dp"/>

    <TextView
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Olim Habaita"
        android:gravity="center"
        android:textSize="20dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/olim_habayta"
        app:layout_constraintBottom_toBottomOf="@id/olim_habayta"
        app:layout_constraintLeft_toLeftOf="@id/olim_habayta"
        app:layout_constraintRight_toRightOf="@id/olim_habayta"/>


    <ImageButton
        android:id="@+id/national_insurance"
        android:layout_width="299dp"
        android:layout_height="85dp"
        android:background="@drawable/info_button"
        app:layout_constraintTop_toBottomOf="@id/olim_habayta"
        android:layout_marginTop="30dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />


    <TextView
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="National Insurance- New Olim"
        android:gravity="center"
        android:textSize="20dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/national_insurance"
        app:layout_constraintBottom_toBottomOf="@id/national_insurance"
        app:layout_constraintLeft_toLeftOf="@id/national_insurance"
        app:layout_constraintRight_toRightOf="@id/national_insurance"/>


    <TextView
        android:id="@+id/titleForm_p1_ref"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="Refugee Rights"
        android:textColor="@color/black"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
